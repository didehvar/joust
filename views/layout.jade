doctype html
html
  head
    title= title

    link(href='/stylesheets/style.css', rel='stylesheet')
  body
    if flash.success
      pre #{flash.success}

    if flash.error
      pre #{flash.error}

    block content

    script(src='//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.1-beta1/jquery.min.js')
    script(src='//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js')
